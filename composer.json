{
    "name": "christhompsontldr/laravel-inky",
    "description": "Foundation Inky email templates in Laravel",
    "keywords": ["foundation", "inky", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Thompson",
            "email": "christhompsontldr@gmail.com"
        }
    ],
    "require": {
        "filipegar/inky": "dev-master",
        "illuminate/support": "~5.1|~6.0|~7.0|~8.0|~9.0",
        "illuminate/view": "~5.1|~6.0|~7.0|~8.0|~9.0",
        "symfony/css-selector": "^2.7|^3.0|^4.0|^6.0",
        "symfony/dom-crawler": "^2.7|^3.0|^4.0|^6.0",
        "tijsverkoyen/css-to-inline-styles": "^2.2"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.3",
        "phpunit/phpunit": "~5.4",
        "mockery/mockery": "^0.9.5"
    },
    "autoload": {
        "psr-4": {
            "Christhompsontldr\\LaravelInky\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Christhompsontldr\\Tests\\LaravelInky\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Christhompsontldr\\LaravelInky\\InkyServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:adriangonzales/inky.git"
        }
    ]
}
